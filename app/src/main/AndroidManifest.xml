<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.survey">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_surveous"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_surveous"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Profile"></activity>
        <activity android:name=".Register" />
        <activity android:name=".Login" />
        <activity android:name=".Chart" />
        <activity android:name=".Survey" />
        <activity android:name=".ActiveTopics" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>